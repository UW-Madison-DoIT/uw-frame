{
  "name": "uw-frame",
  "version": "2.9.0",
  "description": "MyUW's frame project for creating angular based microservices.",
  "scripts": {
    "test": "karma start uw-frame-components/karma.conf.js --single-run",
    "posttest": "cd uw-frame-components",
    "build-all": "npm run build-java && npm run build-static",
    "prebuild-docs": "npm run build-static && npm run clean-docs && mkdir -p target/docs",
    "build-docs": "cp -r target/uw-frame-static/* target/docs && cp -r docs/js docs/img docs/markdown docs/my-app docs/superstatic.json target/docs",
    "postbuild-docs": "htmlprocessor docs/index.html -o target/docs/index.html",
    "prebuild-java": "npm run build-components",
    "build-java": "cd uw-frame-java && mvn clean install",
    "build-components": "node tools/uw-frame-components/build.js",
    "prebuild-static": "mkdir -p target/uw-frame-static && npm run build-components",
    "build-static": "node tools/uw-frame-static/build.js",
    "postbuild-static": "htmlprocessor uw-frame-static/index.html -o target/uw-frame-static/index.html",
    "predocs": "npm run build-docs",
    "docs": "cd target/docs && superstatic",
    "docs:watch": "onchange uw-frame-components/** docs/** -e target/docs -v -- npm run build-docs",
    "docs:dev": "npm run build-docs -s && concurrently -k \"npm run docs -s\" \"npm run docs:watch -s\"",
    "jetty": "cd uw-frame-java && mvn jetty:run",
    "prestatic": "npm run build-static",
    "static": "cd target/uw-frame-static && superstatic --host=0.0.0.0",
    "static:watch": "onchange uw-frame-components/** -i -- npm run build-static",
    "static:dev": "npm run build-static -s && concurrently -k \"npm run static -s\" \"npm run static:watch -s\"",
    "docker": "docker run -d --name frame -p 8009:8009 docker.doit.wisc.edu/myuw/uw-frame-superstatic:latest",
    "build-docker": "docker build -t docker.doit.wisc.edu/myuw/uw-frame-superstatic .",
    "stop-docker": "docker stop frame; docker rm frame;",
    "clean": "npm-run-all clean-java clean-static clean-components clean-docs clean-test",
    "clean-test": "rm -rf uw-frame-components/test_out uw-frame-components/coverage",
    "clean-java": "cd uw-frame-java && mvn clean",
    "clean-static": "rimraf target/uw-frame-static",
    "clean-docs": "rimraf target/docs",
    "clean-components": "rimraf target/uw-frame-components"
  },
  "files": [
    "uw-frame-components",
    "uw-frame-static"
  ],
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://github.com/UW-Madison-DoIT/uw-frame/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/UW-Madison-DoIT/uw-frame.git"
  },
  "keywords": [
    "uw-frame",
    "UW-Madison",
    "frame",
    "angularjs"
  ],
  "contributors": [
    {
      "name": "Tim Levett",
      "email": "tim.levett@wisc.edu"
    },
    {
      "name": "Timothy Vertein"
    },
    {
      "name": "Jared Hanstra",
      "email": "jared.hanstra@wisc.edu"
    }
  ],
  "license": {
    "type": "Apache-2.0",
    "url": "https://github.com/UW-Madison-DoIT/uw-frame/blob/master/LICENSE"
  },
  "bugs": {
    "url": "https://github.com/UW-Madison-DoIT/uw-frame/issues"
  },
  "homepage": "https://github.com/UW-Madison-DoIT/uw-frame",
  "devDependencies": {
    "concurrently": "^3.1.0",
    "graceful-fs": "^4.0.0",
    "htmlprocessor": "^0.2.4",
    "jasmine-core": "^2.3.4",
    "karma": "0.13.21",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-coveralls": "^1.1.2",
    "karma-html-reporter": "0.2.2",
    "karma-htmlfile-reporter": "^0.1.2",
    "karma-jasmine": "^0.3.5",
    "karma-jasmine-html-reporter": "0.2.0",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-requirejs": "0.2.6",
    "less": "1.7.5",
    "lodash": "^4.0.0",
    "npm-run-all": "^3.1.1",
    "onchange": "^3.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "posix": "^4.1.1",
    "recursive-copy": "^2.0.5",
    "requirejs": "^2.2.0",
    "rimraf": "^2.5.4",
    "superstatic": "^4.0.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.6.3",
    "normalize.less": "^1.0.0"
  }
